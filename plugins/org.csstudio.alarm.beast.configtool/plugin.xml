<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="org.csstudio.alarm.beast.configtool.Application">
         </run>
      </application>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.csstudio.alarm.beast.configtool.application"
            name="AlarmConfigTool">
         <property
               name="appName"
               value="AlarmConfigTool">
         </property>
      </product>
   </extension>

</plugin>
